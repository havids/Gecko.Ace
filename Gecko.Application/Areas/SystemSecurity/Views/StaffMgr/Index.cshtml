                                                                            <label class="col-sm-3 control-label no-padding-right" for="form-field-1"> 职称: </label>
                                                                            <div class="col-sm-3">
                                                                                <p id="txtStaffTitle" class="form-control-static"></p>
                                                                            </div>
                                                                        </div>
                                                                        <div class="form-group form-group-sm">
                                                                            <label class="col-sm-3 control-label no-padding-right" for="form-field-1"> 分机号码: </label>
                                                                            <div class="col-sm-3">
                                                                                <p id="txtStaffExtNumber" class="form-control-static"></p>
                                                                            </div>
                                                                            <label class="col-sm-3 control-label no-padding-right" for="form-field-1"> 手机: </label>

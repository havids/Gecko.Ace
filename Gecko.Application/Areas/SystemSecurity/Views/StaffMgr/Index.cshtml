                            <div class="clearfix">
                                <button class="btn btn-white btn-info btn-bold" id="s_btnAdd">
                                    <i class="ace-icon glyphicon  glyphicon-plus bigger-120 blue"></i>
                                    新增
                                </button>
                                <button class="btn btn-white btn-info btn-bold" id="s_btnEdit">
                                    <i class="ace-icon glyphicon glyphicon-edit bigger-120 blue"></i>
                                    编辑
                                </button>
                                <button class="btn btn-white btn-info btn-bold" id="s_btnEditPwd">
                                    <i class="ace-icon glyphicon glyphicon-edit bigger-120 blue"></i>
                                    密码重置
                                </button>
                                <button class="btn btn-white btn-info btn-bold" id="s_btnRole">
                                    <i class="ace-icon glyphicon glyphicon-user bigger-120 blue"></i>
                                    角色
                                </button>
                                <button class="btn btn-white btn-info btn-bold" id="s_btnSecurity">
                                    <i class="ace-icon glyphicon glyphicon-lock bigger-120 blue"></i>
                                    授权
                                </button>
                                <button class="btn btn-white btn-info btn-bold" id="s_btnDel">
                                    <i class="ace-icon glyphicon glyphicon-remove bigger-120 red"></i>
                                    删除
                                </button>
                            </div>

                            <div class="hr dotted"></div>

                            <div>
